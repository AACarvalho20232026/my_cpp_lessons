    #include <iostream>
using namespace std;


struct
{
    string nome;
    int idade;
    string codPostal;
    string rua;
    string cidade;
    string pais;

} pessoa;

int main(){

    pessoa.nome = "Joao";
    pessoa.idade = 20;
    pessoa.codPostal = "1234-567";
    pessoa.rua = "Rua Do Joao";
    pessoa.cidade = "Porto";
    pessoa.pais = "Portugal";


}
