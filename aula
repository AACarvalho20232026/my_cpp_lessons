string letras [2][4]; //bidimencional

string letras2[2][4] = {
    {"A","B","C","D"}, //0
    {"E","F","G","H"}  //1
    //0   1   2   3
};

int main (){

    for (int i = 0; i < 2; i++){
        for (int j = 0; j < 4; j++){
    cout << letras2[i][j] << endl;
    }
}
return 0;
}
