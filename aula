Reference

//Reference is an alias for a variable
string food = "Pizza";
//Create a Reference variable
string &meal = food;
int main(){
    cout << food << "\n";
    cout << &food << "\n";
    cout << meal << "\n";
    cout << &meal << "\n";
    
    return 0;
}
........................................................................................................
pointer

int main(){
    string food = "Pizza";
    string *ptr = &food;
    cout << food << "\n";
    cout << &food << "\n";
    cout << ptr << "\n";
    cout << *ptr << "\n";
    cout << &ptr << "\n";
